<h2>Scribble</h2>

<ul><span><%= link_to "Sign Up"%></span> <span><%= link_to "Sign In"%></span></ul>

<% @posts.each do |post| %>

 <h3><%= link_to post.title %></h3>
 <br />
 <ul>
   <h5><%= post.author %> <%= post.date_time %> <%= post.created_at %></h5>
 </ul>
 <br />
 <h4><%= post.text %></h4>
 <img src="<%= post.img_url %>">

<% end %>
